  r=float(input("enter the rate of interest:"))
  t=float(input("enter the time in years:"))
  CI=p*(pow(1+r/100),t))
  print("principal amount:",p)
  print("interest rate:",r)
  print("time in years:",t)
  print("compound interest;",CI)

